    @Html.Partial("_EditIcon")
    @Html.ScriptBundle(
        "~/Scripts/jquery-1.10.2.min.js",
        "~/Scripts/jquery.validate.min.js",
        "~/Scripts/jquery.validate.unobtrusive.min.js",
        "~/Scripts/jquery.unobtrusive-ajax.min.js",
        "~/Scripts/Custum/css_browser_selector.js",
        "~/Scripts/jquery.mmenu-master/js/jquery.mmenu.min.all.js",
        "~/Scripts/Custum/custum.js"
    )
    @RenderSection("FooterHtml", required: false)
