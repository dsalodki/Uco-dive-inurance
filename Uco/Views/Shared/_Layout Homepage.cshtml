                                </a>
                            </li>
                        </ul>
                        <div class="clear"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="clear"></div>

        <div class="midd_sec">
            <div class="center_div">
                @RenderBody()
            </div>
        </div>
        <div class="mobile_contact">
            @Html.Partial("_SmallFormMobile")
        </div>
        <div class="footer">
            <div class="footer_top1">
                <div class="center_div">
                    <div class="footer_top1_right">
                        @Html.Action("_TextComponent", "PagePart", new { SystemName = "Footer 1" })
                        <div class="clear"></div>
                    </div>
                    <div class="footer_socials">
                        @Html.Action("_TextComponent", "PagePart", new { SystemName = "Footer Social" })


                        <div class="clear"></div>
                    </div>
                    <div class="clear"></div>
                </div>
            </div>
            <div class="footer_top2">
                <div class="center_div">
                    <div class="footer_clm1">
                        <div class="footer_heading1">
                            @Html.Action("_TextComponent", "PagePart", new { SystemName = "Footer 2 - Title" })
                        </div>
                        <div class="footer_links">
                            @Html.Action("_TextComponent", "PagePart", new { SystemName = "Footer 2 - List" })

                        </div>
                    </div>
                    <div class="footer_clm2">
                        <div class="footer_heading2">
                            @Html.Action("_TextComponent", "PagePart", new { SystemName = "Footer 3 - Title" })

                        </div>
                        <div class="footer_links">
                            @Html.Action("_TextComponent", "PagePart", new { SystemName = "Footer 3 - List" })

