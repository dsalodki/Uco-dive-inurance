    <div class="@(RP.GetCurrentSettings().LanguageRTL ? "k-rtl" : "k-ltr") homepage">


        <div id="slider_contact">
            @Html.Partial("_SmallForm")
        </div>
        <div id="for_print_logo" class="dn">
