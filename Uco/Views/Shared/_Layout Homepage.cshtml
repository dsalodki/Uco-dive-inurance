                    <div class="footer_bot_text">
                        @Html.Action("_TextComponent", "PagePart", new { SystemName = "תחתית האתר - טקסט" })

                    </div>
                    <div class="secured_by">
                        @Html.Action("_TextComponent", "PagePart", new { SystemName = "תחתית האתר- אייקון SSL" })

                    </div>
                    <div class="clear"></div>
                </div>
            </div>
