                        @Html.Action("_TextComponent", "PagePart", new { SystemName = "תפריט עליון ירוק" })
                    </div>
                    <div id="mobile_nav">
                        @Html.Action("_TextComponent", "PagePart", new { SystemName = "תפריט עליון ירוק" })
                    </div>
                    <div class="clear"></div>
                    <div class="three_circle">
                        <ul>
                            <li class="first">
                                <a href="@Url.Action("Buy","Insurance")">
                                    @Html.Action("_TextComponent", "PagePart", new { SystemName = "עמוד הבית - עיגול 2" })
                                </a>
                            </li>
                            <li class="second">
