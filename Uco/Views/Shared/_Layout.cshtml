<body class="@(Page.PageType != null ? (string)Page.PageType : "")">
    @RenderSection("BodyStart", false)
    <div class="@(RP.GetCurrentSettings().LanguageRTL ? "k-rtl" : "k-ltr")">
        <div id="slider_contact">
            @Html.Partial("_SmallForm")
        </div>
        <div class="header">
            <div class="yop_bordr_left"></div>
            <div class="center_div">
                @Html.Partial("_Header")
                @Html.Partial("_TopMenu")
            </div>
            @if (IsSectionDefined("bannerFirst"))
            {
                @RenderSection("bannerFirst")
            }
            <div class="banner_bott">
                <div class="center_div">
                    <div class="one">@Html.Action("_Banner", "PagePart", new { Num = 1, BannerGroup = "באנר עליון בעמוד הבית - שמאל", pageId = ViewBag.ID })</div>
                    <div class="two">@Html.Action("_Banner", "PagePart", new { Num = 1, BannerGroup = "באנר עליון בעמוד הבית - ימין", pageId = ViewBag.ID })</div>
                    <div class="clear"></div>
                </div>
            </div>
        </div>

        <div class="midd_part">
            <div class="center_div">
                <div class="right_sec">
                    @Html.Partial("_ContactForm")


                    <div class="right_green_btn1">
                        @Html.Action("_Banner", "PagePart", new { Num = 2, BannerGroup = "באנר ימיני קטן", pageId = ViewBag.ID })
                    </div>

                    <div class="right_sec_text1_part">
                        @RP.Text("IndexRightBannerText")
                    </div>


                    <div class="canon_img">
                        @Html.Action("_Banner", "PagePart", new { Num = 2, BannerGroup = "באנר ימיני ראשון", pageId = ViewBag.ID })
                    </div>


                    <div class="right_sec_text1_part">
                        @RP.Text("IndexRightBannerText2")
                    </div>
                   
                    <div class="canon2">
                        @Html.Action("_Banner", "PagePart", new { Num = 2, BannerGroup = "באנר ימיני שני", pageId = ViewBag.ID })
                    </div>

                  
                </div>
                <div class="left_sec">
                    @RenderBody()
                </div>
                <div class="clear"></div>
            </div>
        </div>
        @Html.Partial("_Footer")
    </div>
    @Html.Partial("_EditIcon")
