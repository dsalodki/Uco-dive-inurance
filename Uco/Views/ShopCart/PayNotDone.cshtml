@model Uco.Models.ShopCart
@{
    Layout = ViewBag.Layout;
    ViewBag.Url = "/cart/PayNotDone";
    ViewBag.Title = "סיום הזמנה";
    ViewBag.ID = 0;
    ViewBag.LanguageRTL = true;
}
<div class="brdcms">
    <ul>
        <li>
            <a href="/">@RP.T("Homepage.Breadcrumb")</a>
        </li>
        <li>
            @ViewBag.Title
        </li>
    </ul>
    <div class="clear"></div>
</div>
<div class="page_main_heading"><h2>תשלום לא התקבל</h2></div>
<div class="artile_text2">
    @if (string.IsNullOrEmpty(ViewBag.PayNotDoneText))
    {
        <div>
            תשלום לא התקבל. נציגינו יצרו איתכם קשר בהקדם האפשרי
        </div>
    }
    else
    {
        @ViewBag.PayDoneText
    }
</div>