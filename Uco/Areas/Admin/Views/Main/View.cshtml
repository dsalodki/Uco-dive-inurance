@model Uco.Models.AbstractPage
@{
    ViewBag.Title = "View";
    Layout = "~/Areas/Admin/Views/Shared/_ActionLayout.cshtml";
}
@if (SF.UseMultiDomain() && !LS.CurrentHttpContext.Request.IsLocal)
{
    <script type="text/javascript">
        parent.changeViewFrame("http://@(RP.GetAdminCurrentSettingsRepository().Domain)/@Model.RouteUrl/@Model.SeoUrlName");
        parent.refreshTreeView();
    </script>
}
else
{
    <script type="text/javascript">
        parent.changeViewFrame("/@Model.RouteUrl/@Model.SeoUrlName");
        parent.refreshTreeView();
    </script>
}
