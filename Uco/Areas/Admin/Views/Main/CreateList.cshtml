@model IEnumerable<Type>
@{
    ViewBag.Title = Html.R("Title");
    Layout = "~/Areas/Admin/Views/Shared/_ActionLayout.cshtml";
}
<div class="action_wrap">
    <h2>@Html.R("CreateList")</h2>

    @using (Html.BeginForm()) {
        foreach (var item in Model)
        {
            @Html.RadioButton("MetadataToken", item.MetadataToken, true)
            @RP.T("Models." + item.Name + ".PageName")
            <br />
        }
        if (Model.Count() != 0)
        {
            @Html.ValidationSummary(true)
            <div class="button_div">
                <input type="submit" class="k-button" value="@Html.R("Save")" />
            </div>
        }
        else
        {
            @Html.R("CantFindChildTypes")
        }
    }
</div>