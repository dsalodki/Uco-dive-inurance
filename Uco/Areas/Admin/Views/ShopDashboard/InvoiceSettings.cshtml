@model Uco.Models.Settings
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutNoTree.cshtml";
}
<h2>@ViewBag.Title</h2>
@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <div class="editor-label">@Html.LabelFor(m => m.ShopInvoiceUse)</div>
    <div class="editor-field">@Html.EditorFor(m => m.ShopInvoiceUse)</div>
    
    <div class="editor-label">@Html.LabelFor(m => m.ShopInvoiceUser)</div>
    <div class="editor-field">@Html.EditorFor(m => m.ShopInvoiceUser)</div>
    
    <div class="editor-label">@Html.LabelFor(m => m.ShopInvoiceKey)</div>
    <div class="editor-field">@Html.EditorFor(m => m.ShopInvoiceKey)</div>
    
    <p class="clear editor-submit"><input type="submit" class="k-button" value="שמור" /></p>
}